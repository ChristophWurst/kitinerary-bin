{
    "name": "nextcloud/kitinerary-bin",
    "description": "KItinerary binary executable",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Christoph Wurst"
        }
    ],
    "require": {
        "nextcloud/kitinerary": "^1.0",
        "ext-json": "*",
        "php": "^7.4 || ^8.0",
        "psr/log": "^1.1|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nextcloud\\KItinerary\\Bin\\": "/src"
        }
